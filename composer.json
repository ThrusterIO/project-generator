{
    "name": "thruster/project-generator",
    "type": "tool",
    "description": "Thruster ProjectGenerator Tool",
    "keywords": ["project-generator", "thruster"],
    "homepage": "https://thruster.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": ">=7.0",
        "cpliakas/git-wrapper": "^1.6",
        "symfony/console": ">=2.8",
        "symfony/finder": ">=2.8",
        "funct/funct": "^1.3",
        "symfony/filesystem": ">=2.8"
    },
    "bin": ["bin/thruster-project"],
    "autoload": {
        "psr-4": { "Thruster\\Tool\\ProjectGenerator\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
